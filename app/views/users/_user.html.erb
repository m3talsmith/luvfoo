 <tr class="<%= cycle('odd', 'even') %>">
   <td><%=h user.login %></td>
   <td><%=h user.email %></td>
   <td><%= user.enabled ? _('yes') : _('no') %>
     <% unless user == current_user %>
       <% if user.enabled %>
         <%= link_to(_('disable'), user_path(user.id), :method => :delete) %>
       <% else %>
         <%= link_to(_('enable'), enable_user_path(user.id), :method => :put) %>
       <% end %>
     <% end %>
   </td>
   <td><%= link_to _('edit roles'), user_admin_roles_path(user) %>]</td>
 </tr>

